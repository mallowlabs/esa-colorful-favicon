{
  "manifest_version": 2,
  "name": "esa-colorful-favicon",
  "version": "0.0.2",
  "description": "Add background color to the favicon of esa.io.",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.esa.io/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "icons/favicon.png"
  ],
  "permissions": [
    "https://*.esa.io/*"
  ]
}
